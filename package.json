    "laravel-mix": "^5.0.5",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.11",
    "sass-loader": "^8.0.2",
    "select2": "^4.0.13",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
