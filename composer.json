{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "^6.0",
        "rosswintle/recaptcha": "dev-master#c73008baf49f70831862040a000e063195c8045c",
        "ext-gd": "*"
    },
	"require-dev": {
		"phpunit/phpunit": "^7.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/rosswintle/recaptcha"
		}
	],
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		 "psr-4": {
			 "Tests\\": "tests/"
		 }
	 },
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
